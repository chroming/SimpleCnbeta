{
  "manifest_version": 2,

  "name": "SimpleCnbeta",
  "description": "简化cnbeta页面插件.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
  "content_scripts": [
    {
      "matches": ["http://www.cnbeta.com/*"],
      //"css": ["mystyles.css"],
      "js": ["cnbeta.js"]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.cnbeta.com/*"
  ]
}