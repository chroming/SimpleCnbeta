{
  "manifest_version": 2,

  "name": "SimpleCnbeta",
  "description": "简化cnbeta页面插件.",
  "version": "1.0",

  "browser_action": {
    "default_icon": {
    "19": "cnbeta19.png",
    "38": "cnbeta38.png"
  }},
  "content_scripts": [
    {
      "matches": ["http://www.cnbeta.com/*"],
      "js": ["cnbeta.js"]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.cnbeta.com/*"
  ]
}