{
  "manifest_version": 2,

  "name": "SimpleCnbeta",
  "description": "简化cnbeta页面插件.",
  "version": "1.1",

  "browser_action": {
    "default_icon": {
      "19": "cnbeta19.png",
      "38": "cnbeta38.png",
      "50": "cnbeta50.png"
  }},
  "content_scripts": [
    {
      "matches": ["http://www.cnbeta.com/*"],
      "js": ["jquery-1.12.2.min.js", "cnbetajQ.js"]
    }
  ],
  "permissions": [
    "tabs",
    "https://www.cnbeta.com/*"
  ]
}